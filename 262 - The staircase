#include<bits/stdc++.h>
using namespace std;

int main() {
    int n, k;
    cin >> n;
    vector<int> a(n+2);
    for(int i = 1; i <= n; i++) cin >> a[i];
    cin >> k;
    vector<int> dp(n+2, INT_MIN);
    dp[0] = 0;
    for(int i = 0; i <= n; i++) {
        for(int j = i+1; j <= min(i+k, n+1); j++) {
            dp[j] = max(dp[j], dp[i] + a[j]);
        }
    }
    cout << dp[n+1] << endl;
    return 0;
}
